DiscordServerButton.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
DiscordServerButton.TextStrokeTransparency = 0.000
DiscordServerButton.TextWrapped = true
DiscordServerButton.MouseButton1Click:Connect(function()
	setclipboard("discord.gg/X9cCnn6UET")
	DiscordServerButton.Text = "Copied"
	wait(2)
	DiscordServerButton.Text = "Discord Server"
end)

