local Player = game:GetService("Players").LocalPlayer
local PlayerThumbnail = "http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username="..Player.Name
local plr = game.Players.LocalPlayer
local UI = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local StrikeHubTitle = Instance.new("TextLabel")
local UIGradient = Instance.new("UIGradient")
local AltControlV1Label = Instance.new("TextLabel")
local DiscordServerButton = Instance.new("TextButton")
local Avatar = Instance.new("ImageLabel")
local WelcomeLabel = Instance.new("TextLabel")
local KickAlt = Instance.new("TextButton")

--Properties:

UI.Name = "UI"
UI.Parent = game.CoreGui
UI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

MainFrame.Name = "MainFrame"
MainFrame.Parent = UI
