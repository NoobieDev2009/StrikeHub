StrikeHubTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
StrikeHubTitle.BackgroundTransparency = 1.000
StrikeHubTitle.Position = UDim2.new(0.305113375, 0, 0.04917714, 0)
StrikeHubTitle.Size = UDim2.new(0, 363, 0, 67)
StrikeHubTitle.Font = Enum.Font.SourceSansBold
StrikeHubTitle.Text = "Strike Hub"
StrikeHubTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
StrikeHubTitle.TextScaled = true
